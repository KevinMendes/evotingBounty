				<openjdk-jre.win64.url>
					https://github.com/adoptium/temurin8-binaries/releases/download/jdk8u312-b07/OpenJDK8U-jre_x64_windows_hotspot_8u312b07.zip
				</openjdk-jre.win64.url>
				<openjdk-jre.win64.sha256>a4623365d70e7bc969e84b7f29b6b2eecb6c0686863ed67651506e2b5adf43b0</openjdk-jre.win64.sha256>

				<!-- docker registry for failsafe testContainers -->
				<docker.registry/>

			</properties>
		</profile>
	</profiles>

</project>
