		<profile>
			<id>property-default-value</id>
			<activation>
				<property>
					<name>!property-inject</name>
				</property>
			</activation>
			<properties>

				<!-- nwjs -->
				<nwjs.win64.name>nwjs-sdk-v0.51.2-win-x64</nwjs.win64.name>
